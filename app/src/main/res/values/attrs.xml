        </attr>
    </declare-styleable>

    <declare-styleable name="CustomSpinnerLargeBlack">
        <attr name="customSpinnerLargeBlackType" format="string">
            <enum name="black_type_service" value="1"></enum>
            <enum name="black_date" value="2"></enum>
            <enum name="black_hour" value="3"></enum>
            <enum name="black_give_service" value="4"></enum>
            <enum name="black_provider" value="5"></enum>

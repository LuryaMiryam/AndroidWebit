<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/black"
    android:orientation="vertical"
    android:paddingTop="8dp">
    <RelativeLayout
        android:id="@+id/al_meet_en_"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/personal_details_r_paymant_marginTop"
        android:orientation="horizontal"
        android:paddingStart="@dimen/personal_details_r_paymant_paddingRight">

        <LinearLayout
            android:layout_width="280dp"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:gravity="start"
            android:layout_centerVertical="true"
            android:orientation="vertical">

            <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                android:id="@+id/cal_que1_2"
                android:layout_width="wrap_content"
                style="@style/t1"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/cal_que1"
                android:text="@string/que_limit_set_app"
                android:textColor="@color/light_gray_2" />
        </LinearLayout>
        <webit.bthereapp.CustemViews.YesOrNo_Black
            android:id="@+id/cal_yn1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:gravity="end"/>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/dairy_calendar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/custom_spinner_ll"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/color1"
            android:paddingEnd="5dp"
            android:paddingStart="5dp"
            android:weightSum="2">

            <LinearLayout
                android:id="@+id/l_out"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1.5">

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    android:id="@+id/al_type_txt"
                    style="@style/t1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="start|center_vertical"
                    android:gravity="start"
                    android:text="@string/meetings_number"
                    android:textColor="@color/color_white" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/l_in"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="end"
                android:weightSum="2">

                <LinearLayout
                    android:id="@+id/c_s_ll33"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="end"
                    android:layout_weight="2">

                    <LinearLayout
                        android:id="@+id/c_s_l"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center_vertical"
                        android:background="@color/light_gray_2"
                        android:gravity="end"
                        android:weightSum="5">

                        <webit.bthereapp.CustemViews.Fonts.CustomLightEditText
                            android:id="@+id/calendar_num_meetings"
                            style="@style/t1"
                            android:layout_width="0dip"
                            android:layout_height="25dp"
                            android:layout_gravity="start|center_vertical"
                            android:layout_weight="5"
                            android:background="@color/light_gray_2"
                            android:gravity="start|center_vertical"
                            android:paddingStart="5dp"
                            android:inputType="number"
                            android:text="3"
                            android:textColor="@color/color1" />

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/custom_spinner_l"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/color1"
            android:paddingEnd="5dp"
            android:paddingStart="5dp"
            android:weightSum="2">
            <LinearLayout
                android:id="@+id/l_ou"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1.5">

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    android:id="@+id/al_type_tx"
                    style="@style/t1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="start|center_vertical"
                    android:gravity="start"
                    android:text="@string/length_of_queues"
                    android:textColor="@color/color_white" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/l_i"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:gravity="end"
                android:weightSum="2">

                <LinearLayout
                    android:id="@+id/c_s_ll3"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="end"
                    android:layout_weight="2">

                    <LinearLayout
                        android:id="@+id/c_s_ll"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start|center_vertical"
                        android:background="@color/light_gray_2"
                        android:gravity="end"
                        android:weightSum="5">

                        <webit.bthereapp.CustemViews.Fonts.CustomLightEditText
                            android:id="@+id/calendar_queues_length"
                            style="@style/t1"
                            android:inputType="number"
                            android:layout_width="0dip"
                            android:layout_height="25dp"
                            android:layout_gravity="start|center_vertical"
                            android:layout_weight="5"
                            android:background="@color/light_gray_2"
                            android:gravity="start|center_vertical"
                            android:paddingStart="5dp"
                            android:text="6"
                            android:textColor="@color/color1" />

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

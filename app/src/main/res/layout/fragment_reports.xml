<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/bg_pic_supplier_x1">


    <webit.bthereapp.CustemViews.Fonts.CustomBoldTextView
        style="@style/h1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="27.5dp"
        android:text="@string/reports"
        android:textColor="@color/light_blue" />

        <RelativeLayout
            android:layout_marginTop="15dp"
            android:layout_marginEnd="24dp"
            android:layout_marginStart="24dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_marginBottom="20dp"
                android:id="@+id/operational_utilization_L"
                android:layout_alignParentStart="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/operational_utilization"
                    android:layout_width="122dp"
                    android:layout_height="122dp"
                    android:layout_marginBottom="6dp">

                    <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                        style="@style/h1"
                        android:layout_width="wrap_content"
                        android:layout_height="33dp"
                        android:text="85%"
                        android:layout_marginTop="47.5dp"
                        android:layout_marginBottom="44.5dp"
                        android:layout_centerHorizontal="true"/>

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:src="@drawable/supplier_circle_pink" />

                </RelativeLayout>

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    style="@style/t1"
                    android:id="@+id/operational_utilization_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/using" />

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    style="@style/t1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/current_year" />
            </LinearLayout>


            <LinearLayout
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="20dp"
                android:id="@+id/customers_l"
                android:layout_marginStart="67.5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/customers"
                    android:layout_width="122dp"
                    android:layout_height="122dp"
                    android:layout_marginBottom="6dp">

                    <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                        style="@style/h1"
                        android:layout_width="wrap_content"
                        android:layout_height="33dp"
                        android:text="176"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="47.5dp"
                        android:layout_marginBottom="44.5dp"/>

                    <ImageView
                        android:background="@drawable/supplier_circle_blue"
                        android:layout_width="150dp"
                        android:layout_height="150dp" />
                </RelativeLayout>

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    style="@style/t1"
                    android:id="@+id/customers_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/clients" />

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    style="@style/t1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/current_year" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/appointments_l"
                android:layout_below="@+id/operational_utilization_L"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/qweues"
                    android:layout_width="122dp"
                    android:layout_height="122dp"
                    android:layout_marginBottom="6dp">

                    <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                        style="@style/h1"
                        android:layout_width="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_height="33dp"
                        android:layout_marginTop="47dp"
                        android:layout_marginBottom="44.5dp"
                        android:text="342" />

                    <ImageView
                        android:background="@drawable/supplier_circle_black"
                        android:layout_width="150dp"
                        android:layout_height="150dp" />
                </RelativeLayout>

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    style="@style/t1"
                    android:id="@+id/qweues_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/appointments" />

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    style="@style/t1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/current_year" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/pay_in_app_l"
                android:layout_alignParentEnd="true"
              android:layout_below="@+id/customers_l"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                android:layout_marginStart="67.5dp"
                android:layout_toEndOf="@+id/appointments_l"
                <RelativeLayout
                    android:id="@+id/pay_in_app"
                    android:layout_width="122dp"
                    android:layout_height="122dp"
                    android:layout_marginBottom="6dp">

                    <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                        style="@style/h1"
                        android:layout_width="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_height="33dp"
                        android:layout_marginStart="45dp"
                        android:layout_marginTop="47.5dp"
                        android:layout_marginBottom="44.5dp"
                        android:text="1750" />

                    <ImageView
                        android:background="@drawable/supplier_circle_gray"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:src="@drawable/supplier_circle_gray" />
                </RelativeLayout>

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    style="@style/t1"
                    android:id="@+id/pay_in_app_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/payment_at_the_app" />

                <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
                    style="@style/t1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/current_year" />
            </LinearLayout>
        </RelativeLayout>



    <webit.bthereapp.CustemViews.Fonts.CustomLightButton
        android:layout_marginTop="20dp"
        android:id="@+id/to_all_reports"
        style="@style/t1"
        android:layout_width="166.5dp"
        android:layout_height="40dp"
        android:textAllCaps="false"
        android:layout_gravity="center"
        android:gravity="center_vertical|center_horizontal"
        android:background="@color/light_blue"
        android:text="@string/to_the_full_reports"
        android:textColor="@color/color_white" />
</LinearLayout>


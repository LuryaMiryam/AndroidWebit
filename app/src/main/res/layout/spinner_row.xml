<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/black"
    android:id="@+id/r_all"
    android:layout_height="wrap_content">
    <View
        style="@style/personal_details_view"
        android:layout_width="match_parent"
        android:layout_height="0.20dp"
        android:background="@color/color_white" />
    <webit.bthereapp.CustemViews.Fonts.CustomLightTextView
        style="@style/t1"
        android:paddingStart="@dimen/personal_details_r_paymant_paddingRight"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:id="@+id/type_text"
        android:gravity="center_vertical"
        android:layout_height="@dimen/row_s_h"
        android:layout_width="@dimen/spinner_w"
        android:textColor="@color/color_white"
        android:text="Header" />
    <ImageView
        android:id="@+id/image"
        android:visibility="gone"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_width="30dp"
        android:src="@drawable/supplier_galaxy_icons_x1_59"
        android:layout_marginEnd="10dp"
        android:layout_height="30dp" />
</RelativeLayout>